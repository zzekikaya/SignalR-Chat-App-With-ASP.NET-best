<configuration>
  <connectionStrings>
    <add name="conStr" connectionString="Data Source=localhost;Initial Catalog=SignalRdb;Integrated Security=True" providerName="System.Data.SqlClient"/>
  </connectionStrings>


  <system.web>
    <customErrors mode="Off"/>
  
    <compilation debug="true" targetFramework="4.6.1"/>
    <httpRuntime targetFramework="4.6.1" executionTimeout="240" maxRequestLength="2048000"/>
 
  </system.web>
  <system.webServer>
    <defaultDocument>
      <files>
        <clear/>
        <add value="Index.aspx"/>
        <add value="Default.htm"/>
        <add value="Default.asp"/>
        <add value="index.htm"/>
        <add value="index.html"/>
        <add value="iisstart.htm"/>
        <add value="default.aspx"/>
      </files>
    </defaultDocument>
    <directoryBrowse enabled="true"/>
  </system.webServer>


</configuration>